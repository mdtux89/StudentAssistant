<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:animateLayoutChanges="true"
    android:id="@+id/exam_detail_container"
    android:background="@color/color_name"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ExamDetailActivity"
    tools:ignore="MergeRootFrame" >

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/emptytext"
        android:text="@string/empty"
        android:textStyle="italic"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:visibility="visible"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true">
    </TextView>

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:animateLayoutChanges="true"
        android:orientation="vertical"
        android:id="@+id/exam_detail_container_inner"
        android:visibility="gone"
        android:background="@color/color_name"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ExamDetailActivity"
        tools:ignore="MergeRootFrame" >

        <TextView
            android:layout_gravity="center_horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/exam_name"
            android:textStyle="italic"
            android:typeface="serif"
            android:textSize="20dp"
            android:layout_marginTop="10dp"
            android:background="@color/color_name"
            ></TextView>

        <View
            android:id="@+id/separator"
            android:layout_width="fill_parent"
            android:layout_marginLeft="@dimen/margin"
            android:layout_marginRight="@dimen/margin"
            android:layout_height="1dp"
            android:background="@color/white"/>

        <RelativeLayout
            android:id="@+id/reloutput1"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:clickable="true"
            android:background="@color/color_name"
            android:layout_gravity="center_horizontal|top">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:id="@+id/output1"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="20dp"
                android:layout_marginLeft="10dp"/>
        </RelativeLayout>


        <LinearLayout
            android:orientation="vertical"
            android:layout_below="@+id/blocks"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="@dimen/margin"
            android:layout_marginRight="@dimen/margin"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/borders2"
            android:layout_gravity="center">

            <RelativeLayout
                android:id="@+id/reloutput2"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="15dp"
                android:clickable="true"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:id="@+id/output2"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="10dp"/>
            </RelativeLayout>
            <View
                android:id="@+id/view2"
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:background="@color/color_name"/>
            <RelativeLayout
                android:id="@+id/reloutput3"
                android:layout_below="@id/reloutput2"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="15dp"
                android:clickable="true"
                android:visibility="gone"
                android:layout_marginRight="15dp"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:id="@+id/output3"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="10dp"/>
            </RelativeLayout>
            <View
                android:id="@+id/view3"
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:background="@color/color_name"/>
            <RelativeLayout
                android:id="@+id/reloutput4"
                android:layout_below="@id/reloutput3"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="15dp"
                android:layout_width="fill_parent"
                android:visibility="gone"
                android:layout_height="wrap_content"
                android:clickable="true"
                android:layout_gravity="center">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:id="@+id/output4"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="10dp"/>
            </RelativeLayout>
            <View
                android:id="@+id/view4"
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:background="@color/color_name"/>
            <RelativeLayout
                android:id="@+id/reloutput5"
                android:layout_below="@id/reloutput4"
                android:clickable="true"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="15dp"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:visibility="gone"
                android:layout_gravity="center">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:id="@+id/output5"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="10dp"/>
            </RelativeLayout>
        </LinearLayout>
    </LinearLayout>

</RelativeLayout><!-- From: file:/home/marco/AndroidStudioProjects/StudentAssistantProject/StudentAssistant/src/main/res/layout/fragment_exam_detail.xml -->